import java.util.Scanner;

public class oddecho {
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        scanner.nextLine();

        for(int i = 0; i < N; i++)
            System.out.println(i + ":");
            if(i%2!=0){
                System.out.println(scanner.nextLine());
            }
            
            scanner.nextLine();
        }
    }
}
